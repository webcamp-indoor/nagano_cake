<table class="table mr-5">
  <thead>
    <tr>
      <th>ジャンル検索</th>
    </tr>
  </thead>
  <tbody>
    <% genres.each do |genre| %>
      <tr>
        <td style="padding-left:30px">
          <%= link_to genre.name, genre_search_admin_items_path(genre_id: genre.id) %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>